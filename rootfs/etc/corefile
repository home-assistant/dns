# This is a fallback config and got overwritten by /usr/share/tempio/corefile

.:53 {
    log
    errors
    loop
    hosts /config/hosts {
        fallthrough
    }
    template ANY AAAA local.hass.io hassio {
        rcode NOERROR
    }
    mdns
    forward . dns://127.0.0.11:53 {
        except local.hass.io
        policy sequential
        health_check 1m
    }
    cache 600
}
